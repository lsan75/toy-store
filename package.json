  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:prod",
    "build:prod": "npm run clean:all && npm run webpack:prod && npm run rollup",
    "build:prod-aot": "npm run clean:all && npm run ngc && npm run webpack:prod-aot && npm run rollup",
    "clean:all": "rimraf dist/ && rimraf src/aot/ && rimraf compiled/",
    "clean:reports": "rimraf coverage/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js --inline --progress --watch --content-base src/ --port 9000",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "pretest": "npm run clean:reports",
    "test": "karma start",
    "test:watch": "karma start config/karma.conf.js --singleRun false",
    "rollup": "./node_modules/.bin/rollup -c config/rollup-config.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config config/webpack.config.js -p",
    "webpack:prod-aot": "cross-env NODE_ENV=production USE_AOT=true webpack --config config/webpack.config.js",
    "json:server": "json-server --watch www/mocks/db.json"
  },
  "engines": {
    "node": ">= 4.4.4",
    "npm": ">= 3"
  }
