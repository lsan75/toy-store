{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "sourceMap": true
    },
    "exclude": [
        "node_modules"
    ],
    "filesGlob": [
        "./www/**/*.ts",
        "./index.ts",
        "!./node_modules/**/*.ts"
    ],
    "awesomeTypescriptLoaderOptions": {
        "resolveGlobs": true,
        "forkChecker": true,
        "useWebpackText": true
    },
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    },
    "files": [
        "./www/js/actions/toy.actions.ts",
        "./www/js/actions/translate.actions.ts",
        "./www/js/actions/user.actions.ts",
        "./www/js/app.module.ts",
        "./www/js/app.routes.ts",
        "./www/js/components/toy.component.spec.ts",
        "./www/js/components/toy.component.ts",
        "./www/js/containers/basket.container.ts",
        "./www/js/containers/checkout.container.ts",
        "./www/js/containers/header.container.ts",
        "./www/js/containers/main.container.ts",
        "./www/js/containers/toy.container.ts",
        "./www/js/helpers/helpers.ts",
        "./www/js/main.ts",
        "./www/js/reducers/index.ts",
        "./www/js/reducers/toy.reducer.ts",
        "./www/js/reducers/translate.reducer.ts",
        "./www/js/reducers/user.reducer.ts",
        "./www/js/services/i18n.service.ts",
        "./www/js/services/toy.service.ts",
        "./index.ts"
    ]
}